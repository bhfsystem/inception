#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  runmany 'docker machine rm -f $1 2>&- || true' plane van hotel snow
  runmany 'plane $1' recycle 'swarm primary'
  runmany '$1 recycle; $1 swarm node' van hotel snow
  docker machine ls
  inception network create inception || true
}

source sub "$BASH_SOURCE" "$@"
