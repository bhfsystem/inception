#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  export CONSUL_SERVER='172.28.128.1'
  runmany 'docker machine rm -f $1 2>&- || true' plane van hotel snow
  runmany '$1 recycle; $1 swarm node' plane van hotel snow
  docker machine ls
  inception network create inception
}

source sub "$BASH_SOURCE" "$@"
