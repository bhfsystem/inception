#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  $INCEPTION media
  $INCEPTION build

  $PLANE docker image rebase
  $PLANE docker image reuse

  $VAN docker image import packer
  $VAN docker image import
  $VAN export
  
  $INCEPTION destroy -f

  $VAN rebuild

  $INCEPTION build
}

source sub "$BASH_SOURCE" "$@"
