#!/usr/bin/env bash

_shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
source "$_shome/exec/inceptionx"

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  eval $($PLANE docker machine env --swarm --no-proxy $PLANE)
  exec docker "$@"
}

source sub "$BASH_SOURCE" "$@"
