#!/usr/bin/env bash

function main {
  local shome="${_inception_home:-"$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"}"
  source "$shome/script/profile"
  source inception_common

  export BASEBOX_INSTANCE='dream'
  export BASEBOX_IP="172.28.128.1"
  export BASEBOX_DOCKER_NETWORK_PREFIX="172.29.2"

  exec limbo "$@"
}

source sub "$BASH_SOURCE" "$@"
