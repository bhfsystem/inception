#!/usr/bin/env bash

function main {
  local shome="${_inception_home:-"$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"}"
  source "$shome/script/profile"

  source inception_common

  local nm_tier="${BASH_SOURCE##*/}"

  if limbo "$@"; then
    case "${1:-}" in
      build|recycle|rebase) 
        limbo update
        limbo swarm node
        limbo update
        limbo docker ps
        limbo docker images -a
        ;;
    esac
  fi
}

source sub-chain dream "$@"
