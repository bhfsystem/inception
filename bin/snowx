#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  source "$shome/exec/inceptionx"

  export BASEBOX_INSTANCE=$SNOW
  export BASEBOX_IP="172.28.129.6"
  export BASEBOX_DOCKER_NETWORK_PREFIX="172.29.16"

  exec limbo vmware "$@"
}

source sub "$BASH_SOURCE" "$@"
