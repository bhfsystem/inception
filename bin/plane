#!/usr/bin/env bash

function main {
  local shome="${_inception_home:-"$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"}"
  source "$shome/script/profile"
  source "$shome/exec/inception"

  exec limbo "${INCEPTION_PROVIDER}" "$@"
}

if [[ -z "${INCEPTION_PROVIDER:-}" ]]; then
  source sub-chain "$BASH_SOURCE" virtualbox "$@"
else
  source sub-chain "$BASH_SOURCE" "$@"
fi
