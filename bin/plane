#!/usr/bin/env bash

export INCEPTION_DREAM="${BASH_SOURCE##*/}"
export BASEBOX_INSTANCE="${INCEPTION_DREAM}"

function main {
  local shome="${_inception_home:-"$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"}"
  source "$shome/script/profile"
  source inception_common

  limbo "$@"
}

source sub-chain dream "$@"
