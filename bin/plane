#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  source "$shome/exec/inception"

  export BASEBOX_INSTANCE=$PLANE

  case "$(uname -s)" in
    Darwin)
      export BASEBOX_IP="172.28.128.3"
      ;;
    Linux)
      export BASEBOX_IP="172.28.128.1"
      ;;
  esac
  export BASEBOX_DOCKER_NETWORK_PREFIX="172.29.3"

  exec limbo virtualbox "$@"
}

source sub "$BASH_SOURCE" "$@"
