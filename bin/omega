#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  source "$shome/exec/akw_common"

  export BASEBOX_INSTANCE="$(basename "$BASH_SOURCE")"
  export BASEBOX_IP="${BASEBOX_NETWORK_PREFIX}.6"
  export BASEBOX_DOCKER_NETWORK_PREFIX="172.204.6"
  exec akw "$@"
}

source sub "$BASH_SOURCE" "$@"
