#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  source "$shome/exec/inception"

  export BASEBOX_INSTANCE='snow'
  export BASEBOX_IP="172.28.128.6"
  export BASEBOX_DOCKER_NETWORK_PREFIX="172.29.6"

  exec limbo virtualbox "$@"
}

source sub "$BASH_SOURCE" "$@"
