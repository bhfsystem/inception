#!/usr/bin/env bash

function main {
  local shome="${_inception_home:-"$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"}"
  source "$shome/script/profile"
  source "$shome/exec/inception"

  export BASEBOX_INSTANCE=$VAN
  export BASEBOX_IP="172.28.129.4"
  export BASEBOX_DOCKER_NETWORK_PREFIX="172.29.14"

  exec limbo vmware "$@"
}

source sub "$BASH_SOURCE" "$@"
